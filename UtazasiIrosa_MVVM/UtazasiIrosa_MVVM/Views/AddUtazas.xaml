<UserControl x:Class="UtazasiIrosa_MVVM.Views.AddUtazas"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UtazasiIrosa_MVVM.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="#FFD29090">
        <StackPanel>
            <Label Content="Utazás neve"></Label>
            <TextBox x:Name="nevtb"></TextBox>
            <Label Content="Jármű"></Label>
            <TextBox x:Name="jarmutb"></TextBox>
            <Label Content="Max Létszám"></Label>
            <TextBox x:Name="mletszamtb"></TextBox>
            <Button x:Name="btnmentes" Content="Mentés" Height="200" Width="463" Click="btnmentes_Click"/>



        </StackPanel>
    </Grid>
</UserControl>
